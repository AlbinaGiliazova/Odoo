<?xml version="1.0" encoding="UTF-8"?>
<record id="view_import_packing_csv_wizard" model="ir.ui.view">
    <field name="name">import.packing.csv.wizard.form</field>
    <field name="model">import.packing.csv.wizard</field>
    <field name="arch" type="xml">
        <form string="Импорт заказов из CSV">
            <group>
                <field name="csv_file"/>
            </group>
            <footer>
                <button
                    name="action_import_csv"
                    string="Импорт"
                    type="object"
                    class="btn-primary"
                    context="{'close_after_action': True}"/>
                <button
                    string="Отмена"
                    special="cancel"
                    class="btn-secondary"/>
            </footer>
        </form>
    </field>
</record>